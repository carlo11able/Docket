<h1>User</h1>

<h3>
<%= link_to 'Lista Labeler', "users/labelers" %>
<%= link_to 'Lista Customer', "users/customers" %>
<%= link_to 'Admin Profile', user_path(User.find(current_user.id))%>
</h3>


<h3>Batches</h3>
<% @photos.each do |photo|%>
    <%@user=User.find(photo.user_id)%>
    <%if photo.url!=nil%>
        <%= image_tag(photo.url, alt: 'Image', class: "img_list") if photo.url %>
    <%else%> 
        <%= image_tag(photo.image.url, alt: 'Image', class: "img_list") if photo.image? %>
    <%end%>
    <h4><%= link_to @user.email, user_path(@user) %> | <%= link_to "Show Photo", photo_path(photo) %></h4>
    <br>
<%end%>




